            <term_definitions code="at0000">
              <items id="description">Communication to improve health literacy and life skills.</items>
              <items id="text">Health education</items>
            </term_definitions>
            <term_definitions code="at0001">
              <items id="description">@ internal @</items>
              <items id="text">Tree</items>
            </term_definitions>
            <term_definitions code="at0002">
              <items id="comment">The 'Topic' could identify a single piece of information or a single skill, or it may be the name of a training course or program that may be delivered over multiple sessions or visits. The name may indicate that the education was transferred to a group or the individual.</items>
              <items id="description">Identification of the topic of health education, by name.</items>
              <items id="text">Topic name</items>
            </term_definitions>
            <term_definitions code="at0003">
              <items id="description">Narrative description about the health education.</items>
              <items id="text">Description</items>
            </term_definitions>
            <term_definitions code="at0004">
              <items id="comment">For example: verbal; demonstration; mobile app; or printed material.</items>
              <items id="description">Method by which the health education was communicated to the individual.</items>
              <items id="text">Method</items>
            </term_definitions>
            <term_definitions code="at0006">
              <items id="description">Education has been planned but no steps have been taken to initiate education.</items>
              <items id="text">Education planned</items>
            </term_definitions>
            <term_definitions code="at0007">
              <items id="description">Appointment for education scheduled.</items>
              <items id="text">Education scheduled</items>
            </term_definitions>
            <term_definitions code="at0008">
              <items id="description">Education session or material provided.</items>
              <items id="text">Education provided</items>
            </term_definitions>
            <term_definitions code="at0010">
              <items id="comment">For example: 'Suspended - Patient too tired to continue'; or 'Not completed - Patient did not attend all 4 visits'.</items>
              <items id="description">Reason that the pathway step for the identified health education was carried out.</items>
              <items id="text">Reason</items>
            </term_definitions>
            <term_definitions code="at0013">
              <items id="description">All planned activities for education have been successfully completed.</items>
              <items id="text">Education completed</items>
            </term_definitions>
            <term_definitions code="at0014">
              <items id="description">Education has been postponed prior to commencement.</items>
              <items id="text">Education postponed</items>
            </term_definitions>
            <term_definitions code="at0015">
              <items id="description">Education has been cancelled prior to commencement.</items>
              <items id="text">Education cancelled</items>
            </term_definitions>
            <term_definitions code="at0016">
              <items id="description">Education session was abandoned before complete.</items>
              <items id="text">Education not completed</items>
            </term_definitions>
            <term_definitions code="at0017">
              <items id="description">Planned education sessions were suspended after commencement.</items>
              <items id="text">Education suspended</items>
            </term_definitions>
            <term_definitions code="at0018">
              <items id="description">Education has been recommended but no steps have been taken to initiate education.</items>
              <items id="text">Education recommended</items>
            </term_definitions>
            <term_definitions code="at0019">
              <items id="comment">Coding with an external terminology is optional. For example: a narrative description about the individual's understanding and skill from the educator's point of view or forwarded by the individual; or 'Passed'/'Not passed' status for an assessment.</items>
              <items id="description">Description of the outcome of health education.</items>
              <items id="text">Outcome</items>
            </term_definitions>
            <term_definitions code="at0020">
              <items id="description">Number of the education session in an multi-session course.</items>
              <items id="text">Session Number</items>
            </term_definitions>
            <term_definitions code="at0021">
              <items id="description">@ internal @</items>
              <items id="text">Tree</items>
            </term_definitions>
            <term_definitions code="at0023">
              <items id="comment">For example: Local hospital departmental infomation or additional metadata to align with FHIR or CIMI equivalents.</items>
              <items id="description">Additional information required to capture local context or to align with other reference models/formalisms.</items>
              <items id="text">Extension</items>
            </term_definitions>
            <term_definitions code="at0025">
              <items id="description">Structured details about the education material provided to the individual.</items>
              <items id="text">Material details</items>
            </term_definitions>
            <term_definitions code="at0026">
              <items id="comment">Only for use in association with the 'Education scheduled' pathway step.</items>
