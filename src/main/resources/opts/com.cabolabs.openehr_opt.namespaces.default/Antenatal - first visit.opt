                <term_definitions code="at0000">
                  <items id="description">Summary or persistent information about an individual's current and past jobs and/or roles.</items>
                  <items id="text">Occupation summary</items>
                </term_definitions>
                <term_definitions code="at0001">
                  <items id="description">@ internal @</items>
                  <items id="text">Tree</items>
                </term_definitions>
                <term_definitions code="at0002">
                  <items id="description">Narrative description about the entire occupation history of the individual.</items>
                  <items id="text">Description</items>
                </term_definitions>
                <term_definitions code="at0003">
                  <items id="comment">An individual may have multiple, concurrent active episodes of an occupation if they have a variety of jobs or roles. For example: carer for 2 days per week and employed in a retail job for 3 days a week; employed part-time to support studies.</items>
                  <items id="description">Structured details about each job or role, both current and past.</items>
                  <items id="text">Occupation episode</items>
                </term_definitions>
                <term_definitions code="at0004">
                  <items id="comment">For example: employed; unemployed; or not in labour force. Coding with a terminology is desirable, where possible. Detail about each occupation can be recorded within the CLUSTER.occupation_record archetype.</items>
                  <items id="description">Statement about the individual's current employment.</items>
                  <items id="text">Employment status</items>
                </term_definitions>
                <term_definitions code="at0005">
                  <items id="description">Additional details about the current jobs or roles, or previous occupation history of an individual.</items>
                  <items id="text">Additional details</items>
                </term_definitions>
                <term_definitions code="at0006">
                  <items id="description">Additional narrative about an individual's current occupation or history of occupations not captured in other fields.</items>
                  <items id="text">Comment</items>
                </term_definitions>
                <term_definitions code="at0007">
