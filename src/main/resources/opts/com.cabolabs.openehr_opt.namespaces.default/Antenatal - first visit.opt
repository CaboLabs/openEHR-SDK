                                <term_definitions code="at0.0.1">
                                  <items id="description">The uterus was examined.</items>
                                  <items id="text">Uterus</items>
                                </term_definitions>
                                <term_definitions code="at0.0.10">
                                  <items id="description">The position of the uterus, relative to the cervix.</items>
                                  <items id="text">Position of the uterus</items>
                                </term_definitions>
                                <term_definitions code="at0.0.11">
                                  <items id="description">The uterine fundus is tipped forward, towards the abdomen.</items>
                                  <items id="text">Anteverted</items>
                                </term_definitions>
                                <term_definitions code="at0.0.12">
                                  <items id="description">The uterus is in a neutral, upright position.</items>
                                  <items id="text">Neutral</items>
                                </term_definitions>
                                <term_definitions code="at0.0.13">
                                  <items id="description">The uterine fundus is tipped backward, towards the back.</items>
                                  <items id="text">Retroverted</items>
                                </term_definitions>
                                <term_definitions code="at0.0.14">
                                  <items id="description">Description about the mobility of the uterus.</items>
                                  <items id="text">Mobility</items>
                                </term_definitions>
                                <term_definitions code="at0.0.15">
                                  <items id="description">The uterus is moveable.</items>
                                  <items id="text">Mobile</items>
                                </term_definitions>
                                <term_definitions code="at0.0.16">
                                  <items id="description">The uterus is immovable.</items>
                                  <items id="text">Fixed</items>
                                </term_definitions>
                                <term_definitions code="at0.0.17">
                                  <items id="description">Narrative description about findings of pain or tenderness of the uterus.</items>
                                  <items id="text">Tenderness</items>
                                </term_definitions>
                                <term_definitions code="at0.0.18">
                                  <items id="description">Narrative description about the texture and firmness of the uterus.</items>
                                  <items id="text">Consistency</items>
                                </term_definitions>
                                <term_definitions code="at0.0.19">
                                  <items id="description">Estimation of the relative amount of liquor present in advanced pregnancy.</items>
                                  <items id="text">Liquor estimation</items>
                                </term_definitions>
                                <term_definitions code="at0.0.2">
                                  <items id="comment">Usually only possible in pregnancy after approximately 20 weeks of gestation.</items>
                                  <items id="description">Measured height of the uterus from symphysis pubis to uterine fundus.</items>
                                  <items id="text">Fundal measurement</items>
                                </term_definitions>
                                <term_definitions code="at0.0.20">
                                  <items id="description">The amount of liquor appears less than expected, compared to a typical pregnancy at the same gestation.</items>
                                  <items id="text">Decreased</items>
                                </term_definitions>
                                <term_definitions code="at0.0.21">
                                  <items id="description">The amount of liquor is as expected, compared to a typical pregnancy at the same gestation.</items>
                                  <items id="text">Normal</items>
                                </term_definitions>
                                <term_definitions code="at0.0.22">
                                  <items id="description">The amount of liquor appears more than expected, compared to a typical pregnancy at the same gestation.</items>
                                  <items id="text">Increased</items>
                                </term_definitions>
                                <term_definitions code="at0.0.23">
                                  <items id="description">The number of fetuses identified on palpation.</items>
                                  <items id="text">Number of babies</items>
                                </term_definitions>
                                <term_definitions code="at0.0.24">
                                  <items id="description">There is one baby present.</items>
                                  <items id="text">Single</items>
                                </term_definitions>
                                <term_definitions code="at0.0.25">
