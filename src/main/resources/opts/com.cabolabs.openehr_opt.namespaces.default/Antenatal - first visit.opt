                                    <term_definitions code="at0000">
                                      <items id="description">Specific findings observed during the abdominal palpation of a single fetus in utero.</items>
                                      <items id="text">Palpation of a fetus (per abdomen)</items>
                                    </term_definitions>
                                    <term_definitions code="at0001">
                                      <items id="description">Relationship of the longitudinal axis of the fetus to the long axis of the mother.</items>
                                      <items id="text">Lie</items>
                                    </term_definitions>
                                    <term_definitions code="at0002">
                                      <items id="description">The fetal longitudinal axis is in alignment with the mother's longitudinal axis.</items>
                                      <items id="text">Longitudinal</items>
                                    </term_definitions>
                                    <term_definitions code="at0003">
                                      <items id="description">The fetal longitudinal axis is at an oblique angle to the mother's longitudinal axis. Sometimes also referred to as 'Unstable Lie'.</items>
                                      <items id="text">Oblique</items>
                                    </term_definitions>
                                    <term_definitions code="at0004">
                                      <items id="description">The fetal longitudinal axis is at right angles to the mother's longitudinal axis.</items>
                                      <items id="text">Transverse</items>
                                    </term_definitions>
                                    <term_definitions code="at0005">
                                      <items id="description">Identification of the presenting part of the fetus, determined by abdominal palpation.</items>
                                      <items id="text">Presentation</items>
                                    </term_definitions>
                                    <term_definitions code="at0006">
                                      <items id="description">The head is the presenting part.</items>
                                      <items id="text">Vertex</items>
                                    </term_definitions>
                                    <term_definitions code="at0007">
                                      <items id="description">The buttock is the presenting part.</items>
                                      <items id="text">Breech</items>
                                    </term_definitions>
                                    <term_definitions code="at0008">
                                      <items id="description">A shoulder is the presenting part.</items>
                                      <items id="text">Shoulder</items>
                                    </term_definitions>
                                    <term_definitions code="at0009">
                                      <items id="description">The face is the presenting part.</items>
                                      <items id="text">Face</items>
                                    </term_definitions>
                                    <term_definitions code="at0010">
                                      <items id="description">The forehead is the presenting part.</items>
                                      <items id="text">Brow</items>
                                    </term_definitions>
                                    <term_definitions code="at0011">
                                      <items id="description">Relationship between the fetal denominator and the maternal pelvis, determined by abdominal palpation.</items>
                                      <items id="text">Position</items>
                                    </term_definitions>
                                    <term_definitions code="at0012">
                                      <items id="description">The fetal occiput is pointing towards the right. Also known as Right Occipito-Lateral (ROL).</items>
                                      <items id="text">Right Occipito-Transverse (ROT)</items>
                                    </term_definitions>
                                    <term_definitions code="at0013">
                                      <items id="description">The fetal occiput is pointing anteriorly and towards the right.</items>
                                      <items id="text">Right Occipito-Anterior (ROA)</items>
                                    </term_definitions>
                                    <term_definitions code="at0014">
                                      <items id="description">The fetal occiput is pointing posteriorly and towards the right.</items>
                                      <items id="text">Right Occipito-Posterior (ROP)</items>
                                    </term_definitions>
                                    <term_definitions code="at0015">
                                      <items id="description">The fetal occiput is pointing anteriorly, towards the pubic symphysis.</items>
                                      <items id="text">Occipito-Anterior (OA)</items>
                                    </term_definitions>
                                    <term_definitions code="at0016">
                                      <items id="description">The fetal occiput is pointing posteriorly, towards the sacrum.</items>
                                      <items id="text">Occipito-Posterior (OP)</items>
                                    </term_definitions>
                                    <term_definitions code="at0017">
                                      <items id="description">The fetal occiput is pointing towards the left. Also known as Left Occcipito-Lateral (LOL).</items>
                                      <items id="text">Left Occipito-Transverse (LOT)</items>
                                    </term_definitions>
                                    <term_definitions code="at0018">
                                      <items id="description">The fetal occiput is pointing anteriorly and towards the left.</items>
                                      <items id="text">Left Occipito-Anterior (LOA)</items>
                                    </term_definitions>
                                    <term_definitions code="at0019">
                                      <items id="description">The fetal occiput is pointing posteriorly and towards the left.</items>
                                      <items id="text">Left Occipito-Posterior (LOP)</items>
                                    </term_definitions>
                                    <term_definitions code="at0020">
                                      <items id="description">The fetal sacrum is pointing towards the right.</items>
                                      <items id="text">Right Sacro-Transverse (RST)</items>
                                    </term_definitions>
                                    <term_definitions code="at0021">
                                      <items id="description">The fetal sacrum is pointing anteriorly and towards the right.</items>
                                      <items id="text">Right Sacro-Anterior (RSA)</items>
                                    </term_definitions>
                                    <term_definitions code="at0022">
                                      <items id="description">The fetal sacrum is pointing posteriorly and towards the right.</items>
                                      <items id="text">Right Sacro-Posterior (RSP)</items>
                                    </term_definitions>
                                    <term_definitions code="at0023">
                                      <items id="description">The fetal sacrum is pointing anteriorly, towards the pubic symphysis.</items>
                                      <items id="text">Sacro-Anterior (SA)</items>
                                    </term_definitions>
                                    <term_definitions code="at0024">
                                      <items id="description">The fetal sacrum is pointing posteriorly, towards the sacrum.</items>
                                      <items id="text">Sacro-Posterior (SP)</items>
                                    </term_definitions>
                                    <term_definitions code="at0025">
                                      <items id="description">The fetal sacrum is pointing towards the left.</items>
                                      <items id="text">Left Sacro-transverse (LST)</items>
                                    </term_definitions>
                                    <term_definitions code="at0026">
                                      <items id="description">The fetal sacrum is pointing anteriorly and towards the left.</items>
                                      <items id="text">Left Sacro-Anterior (LSA)</items>
                                    </term_definitions>
                                    <term_definitions code="at0027">
                                      <items id="description">The fetal sacrum is pointing posteriorly and towards the left.</items>
                                      <items id="text">Left Sacro-Posterior (LSP)</items>
                                    </term_definitions>
                                    <term_definitions code="at0028">
                                      <items id="description">The fetal chin is pointing towards the right.</items>
                                      <items id="text">Right Mento-Transverse (RMT)</items>
                                    </term_definitions>
                                    <term_definitions code="at0029">
                                      <items id="description">The fetal chin is pointing anteriorly and towards the right.</items>
                                      <items id="text">Right Mento-Anterior (RMA)</items>
                                    </term_definitions>
                                    <term_definitions code="at0030">
                                      <items id="description">The fetal chin is pointing posteriorly and towards the right.</items>
                                      <items id="text">Right Mento-Posterior (RMP)</items>
                                    </term_definitions>
                                    <term_definitions code="at0031">
                                      <items id="description">The fetal chin is pointing anteriorly, towards the pubic symphysis.</items>
                                      <items id="text">Mento-Anterior (MA)</items>
                                    </term_definitions>
                                    <term_definitions code="at0032">
                                      <items id="description">The fetal chin is pointing posteriorly, towards the sacrum.</items>
                                      <items id="text">Mento-Posterior (MP)</items>
                                    </term_definitions>
                                    <term_definitions code="at0033">
                                      <items id="description">The fetal chin is pointing towards the left.</items>
                                      <items id="text">Left Mento-Transverse (LMT)</items>
                                    </term_definitions>
                                    <term_definitions code="at0034">
                                      <items id="description">The fetal chin is pointing anteriorly and towards the left.</items>
                                      <items id="text">Left Mento-Anterior (LMA)</items>
                                    </term_definitions>
                                    <term_definitions code="at0035">
                                      <items id="description">The fetal chin is pointing posteriorly and towards the left.</items>
                                      <items id="text">Left Mento-Posterior (LMP)</items>
                                    </term_definitions>
                                    <term_definitions code="at0036">
                                      <items id="description">Description of the relationship of the fetal head and limbs to the body of the fetus, determined by abdominal palpation.</items>
                                      <items id="text">Attitude</items>
                                    </term_definitions>
                                    <term_definitions code="at0037">
                                      <items id="description">The fetus is fully flexed.</items>
                                      <items id="text">Flexed</items>
                                    </term_definitions>
                                    <term_definitions code="at0038">
                                      <items id="description">The fetus is not flexed.</items>
                                      <items id="text">Deflexed</items>
                                    </term_definitions>
                                    <term_definitions code="at0039">
                                      <items id="description">The fetus is extended.</items>
                                      <items id="text">Extended</items>
                                    </term_definitions>
                                    <term_definitions code="at0040">
                                      <items id="description">Description about the engagement of the fetal head in the pelvis.</items>
                                      <items id="text">Engagement description</items>
                                    </term_definitions>
                                    <term_definitions code="at0041">
                                      <items id="description">The presenting part is floating high and mobile above the pelvic brim.</items>
                                      <items id="text">High and mobile</items>
                                    </term_definitions>
                                    <term_definitions code="at0042">
                                      <items id="description">The presenting part is at the pelvic brim.</items>
                                      <items id="text">At pelvic brim</items>
                                    </term_definitions>
                                    <term_definitions code="at0043">
                                      <items id="description">The presenting part is engaged in the pelvis.</items>
                                      <items id="text">Engaged</items>
                                    </term_definitions>
                                    <term_definitions code="at0044">
                                      <items id="description">The estimated proportion of the presenting part which is felt above the pelvic brim.</items>
                                      <items id="text">Engagement estimation</items>
                                    </term_definitions>
                                    <term_definitions code="at0045">
                                      <items id="description">Not engaged; none of the head is below the pelvic brim.</items>
                                      <items id="text">0/5</items>
                                    </term_definitions>
                                    <term_definitions code="at0046">
                                      <items id="description">Approximately 20% of the head is below the pelvic brim.</items>
                                      <items id="text">1/5</items>
                                    </term_definitions>
                                    <term_definitions code="at0047">
                                      <items id="description">Approximately 40% of the head is below the pelvic brim.</items>
                                      <items id="text">2/5</items>
                                    </term_definitions>
                                    <term_definitions code="at0048">
                                      <items id="description">Approximately 60% of the head is below the pelvic brim.</items>
                                      <items id="text">3/5</items>
                                    </term_definitions>
                                    <term_definitions code="at0049">
                                      <items id="description">Approximately 80% of the head is below the pelvic brim.</items>
                                      <items id="text">4/5</items>
                                    </term_definitions>
                                    <term_definitions code="at0050">
                                      <items id="description">Fully engaged; all of the head is below the pelvic brim.</items>
                                      <items id="text">5/5</items>
                                    </term_definitions>
                                    <term_definitions code="at0051">
                                      <items id="description">The relative size of the baby compared to the normal for the estimated gestation.</items>
                                      <items id="text">Relative size</items>
                                    </term_definitions>
                                    <term_definitions code="at0052">
                                      <items id="description">The size of the fetus appears smaller than expected for estimated gestation.</items>
                                      <items id="text">Small for dates</items>
                                    </term_definitions>
                                    <term_definitions code="at0053">
                                      <items id="description">The size of the fetus appears as expected for the estimated gestation.</items>
                                      <items id="text">Normal</items>
                                    </term_definitions>
                                    <term_definitions code="at0054">
                                      <items id="description">The size of the fetus appears larger than expected for the estimated gestation.</items>
                                      <items id="text">Large for dates</items>
                                    </term_definitions>
                                    <term_definitions code="at0055">
                                      <items id="description">The estimated weight of the fetus on palpation.</items>
                                      <items id="text">Estimated weight</items>
                                    </term_definitions>
                                  </children>
