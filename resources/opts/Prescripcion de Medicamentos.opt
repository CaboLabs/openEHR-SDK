            <term_definitions code="at0000">
              <items id="description">Detalles del medicamento, vacuna u otro producto terapéutico junto a sus instrucciones de uso.</items>
              <items id="text">Orden de medicamentos</items>
            </term_definitions>
            <term_definitions code="at0001">
              <items id="description">Instrucción para un medicamento en particular, vacuna u otro producto terapéutico, incluyendo dosis y frecuencia.</items>
              <items id="text">Orden</items>
            </term_definitions>
            <term_definitions code="at0002">
              <items id="description">@ internal @</items>
              <items id="text">Tree</items>
            </term_definitions>
            <term_definitions code="at0003">
              <items id="description">El medicamento, vacuna o producto terapéutico que es ordenado a ser administrado al o usado por el paciente. Debe ser un dato codificado de ser posible.</items>
              <items id="text">Medicamento</items>
            </term_definitions>
            <term_definitions code="at0004">
              <items id="description">Información detallada de los ingredientes, forma farmacéutica y concentración.</items>
              <items id="text">Ingredientes y forma farmacéutica</items>
            </term_definitions>
            <term_definitions code="at0005">
              <items id="description">Descripción de la cantidad y unidades del medicamento, vacuna u otro producto terapéutico a usada o aplicada cada vez que el medicamento es aplicado.</items>
              <items id="text">Dosis</items>
            </term_definitions>
            <term_definitions code="at0006">
              <items id="description">Forma estructurada de la dosis del medicamento indicado.</items>
              <items id="text">Dosis estructurada</items>
            </term_definitions>
            <term_definitions code="at0008">
              <items id="description">Temporización de las dosis, pudiendo incluir frecuencia y detalles como la relación a comidas.</items>
              <items id="text">Temporización</items>
            </term_definitions>
            <term_definitions code="at0009">
              <items id="description">Texto narrativo con la descripción de cuánto, cuándo y cómo aplicar el medicamento, vacuna u otro producto terapéutico.</items>
              <items id="text">Indicaciones generales</items>
            </term_definitions>
            <term_definitions code="at0010">
              <items id="description">Detalles estructurados de temporización del uso o aplicación del medicamento, vacuna u otro producto terapéutico</items>
              <items id="text">Temporización estructurada</items>
            </term_definitions>
            <term_definitions code="at0011">
              <items id="description">Condición bajo la cuál se requiere el comienzo de la aplicación o uso del medicamento.</items>
              <items id="text">Criterio de comienzo</items>
            </term_definitions>
            <term_definitions code="at0012">
              <items id="description">Fecha con tiempo opcional donde se comienza a usar el medicamento, vacuna u otro producto terapéutico.</items>
              <items id="text">Fecha de comienzo</items>
            </term_definitions>
            <term_definitions code="at0013">
              <items id="description">Fecha con tiempo opcional de cuándo se detuvo la aplicación o uso del medicamento.</items>
              <items id="text">Fecha de parada</items>
            </term_definitions>
            <term_definitions code="at0014">
              <items id="description">Periodo de tiempo en el cual debe administrarse o usarse el medicamento.</items>
              <items id="text">Duración del tratamiento</items>
            </term_definitions>
            <term_definitions code="at0015">
              <items id="description">Número total de dosis del medicamento, vacuna o producto terapéutico a ser usado o administrado.</items>
              <items id="text">Número de administraciones</items>
            </term_definitions>
            <term_definitions code="at0016">
              <items id="description">Condición bajo la cual detenerse la administración o uso del medicamento.</items>
              <items id="text">Criterio de finalización</items>
            </term_definitions>
            <term_definitions code="at0017">
              <items id="description">El medicamento es de tratamiento prolongado y va a ser re-prescripto durante un periodo de tiempo.</items>
              <items id="text">Tratamiento prolongado</items>
            </term_definitions>
            <term_definitions code="at0018">
              <items id="description">Motivo para indicar el medicamento.</items>
              <items id="text">Indicaciones clínicas</items>
            </term_definitions>
            <term_definitions code="at0023">
              <items id="description">Información para quien dispensará el medicamento.</items>
              <items id="text">Dispensación</items>
            </term_definitions>
            <term_definitions code="at0025">
              <items id="description">Cantidad de veces que el medicamento debe ser recargado o dispensado sin una nueva prescripción.</items>
              <items id="text">Número de repeticiones</items>
            </term_definitions>
            <term_definitions code="at0026">
              <items id="description">Indica si se permite o no la sustitución del medicamento prescrito, por una marca diferente del mismo medicamento, vacuna o producto terapéutico, donde se han establecido como bioequivalentes, al momento de dispensar o aplicar el medicamento.</items>
              <items id="text">Sustitución de marca permitida</items>
            </term_definitions>
            <term_definitions code="at0028">
              <items id="description">Instrucciones adicionales para la persona que dispensa el medicamento.</items>
              <items id="text">Instrucciones de dispensación</items>
            </term_definitions>
            <term_definitions code="at0029">
              <items id="description">El tiempo de aplicación depende de las condiciones del paciente y de sus síntomas.</items>
              <items id="text">Cuando sea necesario</items>
            </term_definitions>
            <term_definitions code="at0031">
              <items id="description">@ internal @</items>
              <items id="text">Tree</items>
            </term_definitions>
            <term_definitions code="at0032">
              <items id="description">Identificador utilizado en un sistema externo, y asociado con esta prescripción de medicamento.</items>
              <items id="text">Identificador para la instrucción del medicamento</items>
            </term_definitions>
            <term_definitions code="at0035">
              <items id="description">Cualquier información adicional que pueda ser necesaria para asegurar la continuidad del suministro, justificación de la dosis y temporización actuales, o factores de seguridad y uso apropiado del medicamento.</items>
              <items id="text">Comentario</items>
            </term_definitions>
            <term_definitions code="at0037">
              <items id="description">Detalles estructurados sobre el tiempo/frecuencia de uso o administración del medicamento.</items>
              <items id="text">Temporización detallada</items>
            </term_definitions>
            <term_definitions code="at0038">
              <items id="description">Indicación específica para uso, requerido por una agencia de autorización para obtener un subsidio sobre el, o acceso al, medicamento. Puede ser para un programa de medicamentos a nivel nacional, para compañías de seguros u otras agencias financiadoras.</items>
              <items id="text">Indicación para uso autorizado</items>
            </term_definitions>
            <term_definitions code="at0042">
              <items id="description">Indica la categoría de subsidio adecuada al elemento que se prescribe.</items>
              <items id="text">Beneficio de concesión</items>
            </term_definitions>
            <term_definitions code="at0043">
              <items id="description">Detalles de administración del medicamento, vacuna o producto terapéutico.</items>
              <items id="text">Detalles de administración</items>
            </term_definitions>
            <term_definitions code="at0044">
              <items id="description">Declaración adicional de cómo utilizar el medicamento.</items>
              <items id="text">Instrucciones adicionales</items>
            </term_definitions>
            <term_definitions code="at0045">
              <items id="description">Cantidad del medicamento a ser dispensado.</items>
              <items id="text">Cantidad</items>
            </term_definitions>
            <term_definitions code="at0046">
              <items id="description">Intervalo mínimo de tiempo entre repeticiones de dispensación del medicamento.</items>
              <items id="text">Intervalo mínimo entre repeticiones</items>
            </term_definitions>
            <term_definitions code="at0047">
              <items id="comment">Puede utilizarse para indicar que el paciente ha tomado 3 días de un tratamiento de 7 días.</items>
              <items id="description">Duración del tratamiento previo donde se aplicó el mismo medicamento.</items>
              <items id="text">Duración del tratamiento previo</items>
            </term_definitions>
            <term_definitions code="at0048">
              <items id="description">Detalles de los límites de seguridad del medicamento como dosis máxima o frecuencia de uso.</items>
              <items id="text">Límites de seguridad</items>
            </term_definitions>
          </children>
